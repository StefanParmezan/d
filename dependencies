    requires eu.hansolo.tilesfx;
    requires java.net.http;
    requires com.fasterxml.jackson.databind;
    requires jdk.jfr;
    requires com.fasterxml.jackson.datatype.jsr310;
    requires javafx.graphics;
    opens com.stefanparmezan.vendingmarketui.model to com.fasterxml.jackson.databind;
    opens com.stefanparmezan.vendingmarketui to javafx.fxml;
    exports com.stefanparmezan.vendingmarketui;
    exports com.stefanparmezan.vendingmarketui.controllers;
    opens com.stefanparmezan.vendingmarketui.controllers to javafx.fxml;
}
    
